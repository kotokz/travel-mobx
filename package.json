{
  "name": "travel-mobx",
  "version": "1.0.0",
  "description": "Boilerplate for MobX + React project with Typescript, ES6 compilation and hot code reloading",
  "scripts": {
    "start": "ts-node --project server server/index.ts",
    "clean": "rimraf dist",
    "build": "better-npm-run build",
    "deploy": "cross-env NODE_ENV=production node bin/index.js",
    "build:client": "webpack",
    "build:server": "tsc --p server/tsconfig.json"
  },
  "betterScripts": {
    "build": {
      "command": "npm run build:client && npm run build:server",
      "env": {
        "NODE_ENV": "production"
      }
    }
  },
  "keywords": [
    "mox"
  ],
  "author": "Tony Zhou",
  "license": "MIT",
  "devDependencies": {
    "@types/koa": "^2.0.32",
    "@types/koa-router": "^7.0.20",
    "@types/koa-static": "^2.0.19",
    "@types/lodash": "^4.14.34",
    "@types/node": "^6.0.38",
    "@types/react": "^0.14.34",
    "@types/react-bootstrap": "0.0.32",
    "@types/react-dom": "^0.14.16",
    "@types/react-router": "^2.0.33",
    "@types/react-router-bootstrap": "^0.0.25-alpha",
    "@types/webpack": "^1.12.34",
    "better-npm-run": "0.0.11",
    "config": "^1.21.0",
    "cross-env": "^2.0.1",
    "css-loader": "^0.25.0",
    "cssnano": "^3.7.4",
    "extract-text-webpack-plugin": "^2.0.0-beta.3",
    "file-loader": "^0.9.0",
    "html-webpack-plugin": "^2.22.0",
    "json-loader": "^0.5.4",
    "less": "^2.7.1",
    "less-loader": "^2.2.3",
    "postcss-loader": "^0.13.0",
    "react-hot-loader": "^3.0.0-beta.3",
    "rimraf": "^2.5.4",
    "scss-loader": "^0.0.1",
    "style-loader": "^0.13.1",
    "ts-loader": "^0.8.2",
    "ts-node": "^1.3.0",
    "typescript": "^2.1.0-dev.20160917",
    "url-loader": "^0.5.7",
    "webpack": "^2.1.0-beta.22",
    "webpack-dev-middleware": "^1.8.0",
    "webpack-hot-middleware": "^2.12.2",
    "webpack-manifest-plugin": "^1.0.1"
  },
  "dependencies": {
    "koa": "^2.0.0-alpha.7",
    "koa-connect-history-api-fallback": "^0.3.1",
    "koa-convert": "^1.2.0",
    "koa-proxy": "^0.6.0",
    "koa-router": "^5.4.0",
    "koa-static": "^2.0.0",
    "lodash": "^4.15.0",
    "mobx": "^2.5.1",
    "mobx-react": "^3.5.6",
    "mobx-react-devtools": "^4.2.6",
    "moment": "^2.15.0",
    "react": "^15.3.1",
    "react-bootstrap": "^0.30.3",
    "react-dom": "^15.3.1",
    "react-router": "^2.7.0",
    "react-router-bootstrap": "^0.23.1"
  }
}
